{
  "name": "express-backend-template",
  "version": "1.0.0",
  "description": "express-backend-template",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "env-cmd -f ./tests/.env jest --verbose --runInBand --testLocationInResults --passWithNoTests",
    "test:watch": "npm run test -- --watch",
    "start": "env-cmd node server.js",
    "start:watch": "env-cmd nodemon -i ./tests server.js",
    "create-dbs": "env-cmd createdb && env-cmd -f ./tests/.env createdb",
    "setup-db:dev": "env-cmd psql -f ./sql/setup.sql",
    "setup-db:heroku": "heroku pg:psql -f ./sql/setup.sql"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/belliott15/Express-Backend-Template.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/eslint-plugin": "^7.17.7",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@types/jest": "^28.1.1",
    "@types/pg": "^8.6.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3"
  }
}
